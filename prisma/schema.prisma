
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file: ./../../database/database.db"
}

model Users {
  id String  @id @default(uuid())
  name  String?
  email String  @unique
  password String
  admin Boolean @default(false)
  profile_image String? 
}
